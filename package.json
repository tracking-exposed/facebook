{
  "name": "fbTREX",
  "version": "1.1.0",
  "description": "facebook.tracking.exposed â†’ collaborative tool to investigate personalization algorithms",
  "scripts": {
    "collector:prod": "FBTREX=production DEBUG=fbtrex:*,routes:* node bin/collector",
    "collector:watch": "key=fuffa elastic=disabled DEBUG=lib:*,fbtrex:*,routes:*,-lib:mongo:read nodemon bin/collector.js",
    "content:prod": "FBTREX=production DEBUG=fbtrex:*,routes:* node bin/content",
    "content:watch": "key=fuffa elastic=disabled DEBUG=lib:*,fbtrex:*,routes:* nodemon bin/content.js --config config/nodemon.json",
    "parserv": "elastic=disabled DEBUG=*,-lib:parse,-lib:mongo:* node bin/parserv.js",
    "semanticserv": "elastic=disabled DEBUG=*,-lib:mongo*,-lib:semantic:v node bin/semanticserv.js",
    "test": "./node_modules/.bin/mocha"
  },
  "author": "Claudio Agosti <claudio@tracking.exposed>, https://github.com/tracking-exposed/facebook/graphs/contributors",
  "license": "AGPL-3.0",
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.18.3",
    "bootstrap": ">=4.3.1",
    "bs58": "^3.1.0",
    "c3": "^0.4.11",
    "cookie": "^0.3.1",
    "cors": "^2.8.1",
    "d3": "^3.5.17",
    "debug": "^2.2.0",
    "elasticsearch": "^15.2.0",
    "express": "^4.16.3",
    "food-words": "^1.1.0",
    "harmony-reflect": "^1.6.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "nacl-signature": "^1.0.0",
    "nconf": "^0.8.4",
    "node-geoip": "^1.0.1",
    "request": "^2.88.0",
    "reveal.js": "^3.7.0",
    "rss": "^1.2.2",
    "tweetnacl": "^0.14.5",
    "tweetnacl-util": "^0.15.0",
    "mongodb2": "npm:mongodb@^2.1.8",
    "mongodb3": "npm:mongodb@3.2.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.9",
    "jsdom": "^13.0.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0"
  }
}
