{
    "mongoHost": "localhost",
    "mongoPort": "27017",
    "mongoDb": "facebook",

    "stats": [
    {
        "name": "supporters",
        "column": "supporters2",
        "timevar": "keyTime",
        "variables": [
            {
                "name": "newcomers",
                "selector": {}
            }
        ]
    },
    {
        "name": "active",
        "column": "supporters2",
        "timevar": "lastActivity",
        "variables": [
            {
                "name": "active",
                "selector": {}
            }
        ]
    },
    {
        "name": "timelines",
        "column": "timelines2",
        "timevar": "startTime",
        "variables": [
            {
                "name": "skipped",
                "selector": { "nonfeed": true}
            },
            {
                "name": "newsfeed",
                "selector": { "nonfeed": { "$exists": false }}
            },
            {
                "name": "total",
                "selector": {}
            }
        ]
    },
    {
        "name": "impressions",
        "column": "impressions2",
        "timevar": "impressionTime",
        "variables": [
            {
                "name": "private",
                "selector": { "visibility": "private" }
            },
            {
                "name": "public",
                "selector": { "htmlId": { "$exists": true} }
            },
            {
                "name": "total",
                "selector": { }
            }
        ]
    },
    {
        "name": "processing",
        "column": "htmls2",
        "timevar": "savingTime",
        "variables": [
            {
                "name": "successful",
                "selector": { "processed": true }
            },
            {
                "name": "failure",
                "selector": { "processed": false }
            },
            {
                "name": "unprocessed",
                "selector": { "processed": { "$exists": false } }
            },
            {
                "name": "total",
                "selector": {}
            }
        ]
    },
    {
        "name": "metadata",
        "column": "metadata2",
        "timevar": "impressionTime",
        "variables": [
            {
                "name":"complete",
                "selector": { "complete": true }
            },
            {
                "name":"hasNature",
                "selector": { "nature.kind": { "$ne": null }}
            },
            {
                "name":"hasLinks",
                "selector": { "meaningfulId.links[0].id": { "$exists": true }}
            },
            {
                "name":"hasPictures",
                "selector": { "meaningfulId.images[0].id": { "$exists": true }}
            },
            {
                "name":"hasTexts",
                "selector": { "texts[0]": { "$exists": true }}
            },
            {
                "name":"total",
                "selector": {}
            }
        ]
    }
  ]
}
